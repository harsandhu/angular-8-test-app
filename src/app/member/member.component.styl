.member-page
    display flex
    flex-direction row
    width 100%
    flex-basis 0
    height calc(100vh - 40px)
    .member-detail,.votes-detail
        padding 10px
        width 100%
        display flex
        height 100%
    .member-detail
        background bisque
        border-right 6px solid lightcoral
    .votes-detail
        background lightblue
        position relative

.voter
    overflow-y auto
    width 100%