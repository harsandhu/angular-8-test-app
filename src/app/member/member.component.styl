.member-page
    display flex
    flex-direction row
    width 100%
    flex-basis 0
    height calc(100vh - 40px)
    .member-detail,.votes-detail
        padding 10px
        width 100%
        display flex
        height 100%
    .member-detail
        background bisque
        border-right 6px solid lightcoral
        flex-direction column
        .member 
            display flex
            flex-wrap wrap 
            align-items center
            justify-content center
            flex-direction column
            .image-card
                height 200px
                width 200px
                border 5px solid #f08080
                margin 10px
                .fa
                    font-size 180px
            .properties
                display flex
                flex-direction column
                justify-content center 
                align-items baseline
                .line
                    display flex
                    align-items center
                    width 100%
                    .value 
                        padding-left 15px
                        color darkred
    .votes-detail
        background lightblue
        position relative

.voter
    overflow-y auto
    width 100%